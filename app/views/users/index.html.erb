<h1>All users</h1>
<ul class="users">
	<% @users.each do |user| %>
	  <li><p>User id:<%= user.id %></p></li>
	  <li><p>First name: <%= link_to user.firstname, user_path(user) %></p></li>
	  <li><p>Last name: <%= user.lastname %></p></li>
	  <li><p>Email: <%= user.email %></p></li>
	  <li><p>Location:<%= user.location %></p></li>
	  </br>
	<% end %>
</ul>